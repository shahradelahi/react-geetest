{
  "name": "react-geetest-v4",
  "version": "1.1.2",
  "description": "React library for GeeTest captcha v4 integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"**/*.{ts,md}\"",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run format:check && pnpm run typecheck && pnpm run build"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahradelahi/react-geetest.git"
  },
  "bugs": {
    "url": "https://github.com/shahradelahi/react-geetest/issues"
  },
  "keywords": [
    "react",
    "geetest",
    "captcha"
  ],
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "license": "MIT",
  "packageManager": "pnpm@8.11.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.35",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "tsup": "^8.0.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "crypto-js": "^4.1.1"
  }
}
