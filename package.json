{
  "name": "react-geetest-v4",
  "version": "1.1.3",
  "description": "React library for GeeTest captcha v4 integration",
  "keywords": [
    "react",
    "geetest",
    "captcha"
  ],
  "homepage": "https://github.com/shahradelahi/react-geetest",
  "repository": "github:shahradelahi/react-geetest",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm build && pnpm format:check && pnpm typecheck",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "dependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@shahrad/prettier-config": "^1.2.2",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.35",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^3.7.4",
    "tsup": "^8.0.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "packageManager": "pnpm@8.11.0"
}
